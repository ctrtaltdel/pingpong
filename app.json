      "package": "com.oh_its_ryan_apparently.ping_pong"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6b9ef2dc-1c39-4bd3-a00e-24431c2cf2e4"
      }
